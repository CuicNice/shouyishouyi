/* pages/search/search.wxss */
/* 背景改动 */
page::before {
  content: ' ';
  position: fixed;
  z-index: -1;
  margin-top: 686rpx;
  height: 774rpx;
  width: 749rpx;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  opacity: 0.5;
  background-position-x: right;
  background-position-y: bottom;
  background: url(http://m.qpic.cn/psc?/V12Wfpcu1ZlWOw/TmEUgtj9EK6.7V8ajmQrEKnmDWgLb*qKUItOYRNGl2.5sYIdauCM5l8QmGyx6j4jCYHmtlkN6rG0igAUKdXdfi7xdFSzKv6IQ52fEB70KH4!/b&bo=7QIGAwAAAAADJ.g!&rf=viewer_4&t=5) center 0 no-repeat;
}

/* 搜索列表 */
.search-list {
  margin-left: 38rpx;
  margin-top: 42rpx;
  display: flex;
  justify-content: space-between;
}

/* 左边样式 */
.search-left {
  width: 101rpx;
  height: 100rpx;
  border-radius: 11rpx;
  border: 1rpx solid #000000;
}

.search-left-top {
  width: 101rpx;
  height: 30rpx;
  padding-bottom: 12rpx;
  background: #454545;
  border-radius: 11rpx;
  border: 1rpx solid #000000;
  margin-left: -1rpx;
  margin-top: -1rpx;
}

.searchtextbox {
  width: 65rpx;
  height: 38rpx;
  margin-left: 22rpx;
  display: flex;
  align-items: center;
}

.search-left-top-text {
  font-size: 27rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  line-height: 38rpx;
}

.search-left-bottom {
  margin-left: 11rpx;
  width: 76rpx;
  height: 46rpx;
  font-size: 33rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 900;
  color: #000000;
  line-height: 46rpx;
}

/* 右边样式 */
.search-right {
  margin-left: 17rpx;
}

.search-right-top {
  width: 574rpx;
  height: 38rpx;
  font-size: 27rpx;
  margin-right: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 900;
  color: #000000;
  line-height: 38rpx;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  word-break: break-all;
  overflow: hidden;
  text-overflow: ellipsis;

}

.search-right-bottom {
  margin-top: 4rpx;
  display: flex;
  justify-content: space-between;
}

.search-right-bottom-text {
  width: 445rpx;
  height: 52rpx;
  font-size: 19rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #736E6E;
  line-height: 26rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  word-break: break-all;
  overflow: hidden;
  text-overflow: ellipsis;
}

.search-right-bottom-type {
  margin-top: 29rpx;
  margin-right: 20rpx;
  width: 114rpx;
  height: 26rpx;
  font-size: 19rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  line-height: 26rpx;
}

.high {
  color: #f00;
}

.unfind {
  padding: 30rpx;
}