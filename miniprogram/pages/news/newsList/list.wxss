/* pages/list.wxss */


/* 导航标题 */

page {
  overflow: hidden;
}


/* 背景颜色 */

.bg {
  /* Entering for the first time */
  position: absolute;
  width: 100%;
  height: 100%;
  /* 背景 */
  background: linear-gradient(180.04deg, #B9EFE1 0.04%, #FCFCFC 30%);
  z-index: -999;
}


/* 背景的物体 */

.bgThingsBox {
  position: fixed;
  left: 254.66rpx;
  top: 532rpx;
  opacity: 0.2;
}

.bgThings {
  width: 178.74 !important;
  height: 881.48rpx !important;
}

.navigationBar {
  height: 101rpx;
  display: flex;
  justify-content: start;
  /* margin-top: 10rpx; */
}


/* 搜索 */


/* 提示文字 */

.searchHolder {
  /* 输入你要搜索的图书 */
  width: 280rpx;
  height: 40rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
  /* identical to box height */
  text-transform: capitalize;
  color: #000000;
  opacity: 0.6;
  /* Inside auto layout */
  flex: none;
  order: 0;
  flex-grow: 0;
}


/* 需要固定 */

.searchBarBox {
  /* searchbar */
  position: fixed;
  width: 622rpx;
  margin-top: 40rpx;
  box-sizing: border-box;
  /* Auto layout */
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 40rpx;
  gap: 20rpx;
  margin-left: 64rpx;
  margin-right: 64rpx;
  height: 80rpx;
  background: #FFFFFF;
  border: 2rpx solid #20C38C;
  border-radius: 164rpx;
}


/* 固定 */

.searchContent {
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
  /* identical to box height */
  text-transform: capitalize;
  color: #000000;
  opacity: 0.6;
  /* Inside auto layout */
  flex: none;
  order: 0;
  flex-grow: 0;
}


/* 底部的新闻group */

.textInfo {}

.searchIconBox {
  /* icon/search */
  width: 40rpx;
  height: 40rpx;
}

.searchIcon {
  /* Inside auto layout */
  width: 40rpx !important;
  height: 40rpx !important;
  /* Inside auto layout */
  flex: none;
  order: 1;
  flex-grow: 0;
}

@keyframes box-ani {
  from {
    transform: rotate(0)
  }

  to {
    transform: rotate(360deg)
  }
}

.icoBtnA {
  margin-left: 34rpx;
  width: 62rpx;
  height: 62rpx;
  background: #FFFFFF;
  border-radius: 31rpx;
  /* border: 1rpx solid #B8B8B8; */
  display: flex;
  justify-content: center;
  /* 设置弹性盒子元素在主轴的对齐方式，这里direction是column只设置垂直居中 */
  align-items: center;
  /* 子元素水平居中显示 */
  flex-direction: column;
  /* 垂直还是水平排列 */
  overflow: cover;
  border-radius: 50%;
  position: relative;
  /* margin-left: -30rpx; */
  animation: box-ani 5s infinite linear;
}

.icoBtnB {
  margin-left: 34rpx;
  width: 62rpx;
  height: 62rpx;
  background: #FFFFFF;
  border-radius: 31rpx;
  /* border: 1rpx solid #B8B8B8; */
  display: flex;
  justify-content: center;
  /* 设置弹性盒子元素在主轴的对齐方式，这里direction是column只设置垂直居中 */
  align-items: center;
  /* 子元素水平居中显示 */
  flex-direction: column;
  /* 垂直还是水平排列 */
  overflow: cover;
  border-radius: 50%;
}

.avatarUrl {
  width: 62rpx;
  height: 62rpx;
  display: block;
  overflow: hidden;
  border-radius: 50%;
}


/*搜索框  */

.searchBar {
  margin-left: 15rpx;
  width: 391rpx;
  height: 62rpx;
  background: rgba(147, 149, 151, 0.2);
  border-radius: 31rpx;
  /* 
  透明度需要修改 */
  display: flex;
  /* 水平方向居中 */
  /* justify-content: center; */
  /* 设置弹性盒子元素在主轴的对齐方式，这里direction是column只设置垂直居中 */
  align-items: center;
  /* 子元素水平居中显示 */
  flex-direction: row;
  /* 垂直还是水平排列 */
  overflow: hidden;
}

.searchBtn {
  /* width: 24.3rpx;
  height: 25.2rpx; */
  margin-left: 22rpx;
  display: flex;
  float: left;
  overflow: hidden;
