/* pages/inDetail/inDetail.wxss */

@import '../../../wxParse/wxParse.wxss';

/* 背景 */


/* 背景颜色 */

.bg {
  /* Entering for the first time */
  position: absolute;
  width: 100%;
  height: 100%;
  /* 背景 */
  background: linear-gradient(180.04deg, #B9EFE1 0.04%, #FCFCFC 30%);
  z-index: -999;
}


/* 弹窗组件确定键 */

.dialogCertainButon {
  margin-top: 60rpx;
  display: flex !important;
  flex-direction: row !important;
  justify-content: center !important;
  align-items: center !important;
  padding: 20rpx 28rpx !important;
  gap: 20rpx !important;
  width: 500rpx !important;
  height: 84rpx !important;
  overflow: visible !important;
  /* #20C38C */
  background: #20C38C !important;
  border-radius: 70rpx !important;
  /* Inside auto layout */
  flex: none;
  order: 0;
  flex-grow: 1;
}

.top {
  /* 距离顶部的距离 */
  width: 660rpx !important;
  height: 118.76rpx !important;
  overflow: visible;
}

.topTitleSvg {
  width: 660rpx !important;
  height: 118.76rpx !important;
}


/*  顶部的图片*/

.topSvg {
  display: block;
  width: 660rpx !important;
  height: 118.76rpx !important;
  /* 需要优先级高过wxparse里面的优先级 */
  /* overflow:visible !important; */
}


/* 背景的物体 */

.bgThingsBox {
  position: fixed;
  left: 254.66rpx;
  top: 532rpx;
}

.bgThings {
  width: 178.74 !important;
  height: 881.48rpx !important;
}

.one_page {
  min-height: 100vh;
  background-repeat: no-repeat;
  background-position-x: right;
  background-position-y: bottom;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  overflow: scroll;
}

.cont {
  display: flex;
  /* 对齐的轴列 */
  flex-direction: column;
  align-items: center;
  /* height: 100%; */
  margin: 0 auto;
  padding: 58rpx 0 86rpx;
}

.background {
  width: 100%;
  height: 100%;
  margin-left: 85rpx;
  margin-top: 83rpx;
}

.border {
  /* 由于使用的wxss解析插件，所以文本部分的宽度只能够使用宽度来解决 */
  display: block;
  /* padding内边框，外边框margin:10rpx 12rpx 14rpx 16rpx;  意思上元素块为10rpx，右元素块距离为12rpx ，下元素块距离为14rpx，左元素块距离16rpx */
  width: 622rpx;
  padding: 40rpx 29rpx;
  /*关键点 利用边距撑大容器,使得背景图片也跟着变大变小.而不是给这个元素定个固定的高度.*/
  margin: 40rpx 0rpx 17.24rpx 0rpx;
  /* 长度根据内容多少决定 */
  /* height: 867rpx; */
  background: rgba(255, 255, 255, 0.7);
  border-radius: 26rpx;
}


/* 虚线 */

.boderLine {
  box-sizing: border-box;
  /* Auto layout */
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
  padding: 0rpx 0rpx 22rpx;
  gap: 20rpx;
  width: 622rpx;
  border-bottom: 2rpx dashed rgba(0, 0, 0, 0.4);
  /* Inside auto layout */
  flex: none;
  order: 1;
  align-self: stretch;
  flex-grow: 0;
}

.title {
  width: 279px;
  /* height: 82rpx; */
  text-align: center;
  font-size: 29rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 900;
  color: #2E2C2C;
  line-height: 41rpx;
  letter-spacing: 2rpx;
  margin: 0 auto;
}

.title_small {
  text-align: center;
  width: 106rpx;
  height: 20rpx;
  font-size: 14rpx;
  font-family: PingFangSC-Light, PingFang SC;
  font-weight: 300;
  color: #2E2C2C;
  line-height: 20rpx;
  letter-spacing: 1rpx;
  margin-top: 17rpx;
  margin-left: 105rpx;
}

.letter {
  /* padding: 0rpx 30rpx; */
  /* width: 311px;
  height: 353px; */
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 12px;
  line-height: 17px;
  text-align: justify;
  text-transform: capitalize;
  color: rgba(41, 41, 69, 0.8);
  /* Inside auto layout */
  flex: none;
  order: 3;
  align-self: stretch;
  flex-grow: 0;
}

.object {
  font-size: 23rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #2E2C2C;
  line-height: 38rpx;
  letter-spacing: 1rpx;
}

.text {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 23rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #2E2C2C;
  line-height: 38rpx;
  letter-spacing: 1rpx;
  margin-top: 30rpx;
}

.signdowns {
  width: 546rpx;
  height: 86rpx;
  margin-top: 34rpx;
}

.signdown {
  line-height: 38rpx;
  text-align: right;
  font-size: 23rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #2E2C2C;
  letter-spacing: 2rpx;
}

.ad {
  /* 更多讯息请微信关注：南李路二十二号 */
  width: 476rpx;
  height: 40rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
  /* identical to box height */
  text-transform: capitalize;
  margin: 46rpx auto 0;
  color: #292945;
  align-self: center;
  /* Inside auto layout */
  flex: none;
  order: 2;
  flex-grow: 0;
}

.bottom {
  display: flex;
  box-sizing: border-box;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0rpx 20rpx 40rpx;
  gap: 148rpx;
  width: 680rpx;
  height: 228rpx;
  background: #FFFFFF;
  opacity: 0.8;
  border-radius: 20rpx;
}


