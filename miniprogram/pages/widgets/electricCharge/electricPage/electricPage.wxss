/* 波浪动画 */
.water {
  position: absolute;
  transform: translateY(500%);
  /* height: 100rpx; */
  width: 100%;
  z-index: -10;
}

.water-c {
  position: relative;
  top: -180rpx;
}

.water-1 {
  height: 200rpx;
  width: 750rpx;
  margin-top: -10rpx;
  background: url("data:image/svg+xml;base64,") repeat-x;
  background-size: 750rpx;
  -webkit-animation: wave-animation-1 2s infinite linear;
  animation: wave-animation-1 2s infinite linear;
}


.water-2 {
  height: 182rpx;
  top: 6rpx;
  background: url("data:image/svg+xml;base64,") repeat-x;
  
  background-size: 752rpx;
  -webkit-animation: wave-animation-2 4s infinite linear;
  animation: wave-animation-2 4s infinite linear;
}

.water-1,
.water-2 {
  position: absolute;
  width: 100%;
  /* height:100rpx; */
}

@keyframes wave-animation-1 {
  0% {
    background-position: 0 top;
  }

  100% {
    background-position: 750rpx  top;
  }
}

@keyframes wave-animation-2 {
  0% {
    background-position: 0 top;
  }

  100% {
    background-position: 750rpx top;
  }
}

