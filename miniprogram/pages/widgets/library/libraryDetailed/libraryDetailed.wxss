/*图书馆搜索界面css*/
.mypageTitle {
  margin: 0 auto;
  font-weight: 500;
  color: #000000;
}

.page {
  position: absolute;
  top: 0rpx;
  width: 100%;
  height: 100%;
  background: linear-gradient(180.04deg, #B9EFE1 0.1%, #FCFCFC 30%);
  z-index: -1;
}

.classForm {
  margin-top: 14rpx;
  width: 470rpx;
  height: 40rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
  text-transform: capitalize;
  color: #292945;
  opacity: 0.6;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.classFormplace {
  margin-top: 14rpx;
  width: 470rpx;
  height: 40rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
  text-transform: capitalize;
  color: #292945;
  opacity: 0.6;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.placeBox {
  margin-left: -12rpx;
  margin-top: 8rpx;
  width: 470rpx;
  height: 40rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
}

.place {
  padding: 0px 9px;
  margin-left: 20rpx;
  gap: 20rpx;
  width: 58rpx;
  height: 40rpx;
  background: #20C38C;
  opacity: 0.6;
  border-radius: 120rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
  text-align: center;
  color: #FFFFFF;
}

.bookNum {
  margin-top: 6rpx;
  margin-left: 107rpx;
  width: 100rpx;
  height: 104rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 74rpx;
  text-align: center;
  line-height: 104rpx;
  color: #20C38C;
}

.bookText {
  width: 100rpx;
  height: 40rpx;
  margin-top: 8rpx;
  margin-left: 105rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 29rpx;
  line-height: 40rpx;
  text-align: center;
  letter-spacing: 5rpx;
  text-transform: capitalize;
  color: #292945;
}

.borrowBox {
  float: left;
  position: relative;
  top: -8rpx;
}

.imag {
  width: 40rpx;
  height: 40rpx;
  float: left;
  position: relative;
  right: 10rpx;
}

.borrow {
  margin-top: 14rpx;
  margin-left: 60rpx;
  gap: 20rpx;
  width: 146rpx;
  height: 40rpx;
  background: rgba(41, 41, 69, 0.6);
  opacity: 0.6;
  border-radius: 120rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
  text-transform: capitalize;
  text-align: center;
  color: #FFFFFF;
}

.bookXQ {
  padding: 30rpx 24rpx;
  gap: 20rpx;
  width: 640rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 4rpx 20rpx rgba(32, 195, 140, 0.3);
  border-radius: 20rpx;
}

.bookBox {
  margin-top: 60rpx;
  display: flex;
  justify-content: center;
}

.bookName {
  width: 632rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 500;
  font-size: 32rpx;
  line-height: 44rpx;
  text-transform: capitalize;
  color: #292945;
}

.all {
  display: flex;
  margin-top: 20rpx;
  width: 640rpx;
  padding-bottom: 50rpx;
  border-bottom: 3rpx dashed rgba(41, 41, 69, 0.1);
}

.picture {
  margin-left: 70rpx;
  float: right;
  width: 150rpx;
  height: 200rpx;
  background: #FFFFFF;
  box-shadow: 0px 1px 4px rgba(0, 0, 0, 0.16);
  border-radius: 5px;
}

.leftall {
  float: left;
  width: 426rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.box {
  position: relative;
  top: -5rpx;
  width: 100%;
  padding-top: 12rpx;
}

.textPart {
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 24rpx;
  line-height: 34rpx;
  text-transform: capitalize;
  color: #292945;
  float: left;
  letter-spacing: 5rpx;
}

.textParts {
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 24rpx;
  line-height: 34rpx;
  text-transform: capitalize;
  color: #7F7F8F;
  letter-spacing: 5rpx;
}

.bottom {
  display: flex;
  margin-top: 35rpx;
  height: 40rpx;
}

.gen {
  position: relative;
  left: 33rpx;
  margin-top: 44rpx;
  padding: 20rpx 24rpx;
  gap: 20rpx;
  width: 640rpx;
  background: #FFFFFF;
  box-shadow: 0rpx 4rpx 20rpx rgba(32, 195, 140, 0.3);
  border-radius: 20rpx;
}

.genTop {
  height: 122rpx;
  width: 610rpx;
