/*图书馆搜索界面css*/
.mypageTitle {
  margin: 0 auto;
  font-weight: 500;
  color: #000000;
}

.page {
  position: absolute;
  top: 0rpx;
  width: 100%;
  height: 100%;
  background: linear-gradient(180.04deg, #B9EFE1 0.1%, #FCFCFC 30%);
  z-index: -1;
}

.bigBox {
  margin-top: 65rpx;
}

.search {
  width: 541rpx;
  height: 41rpx;
  border-radius: 164rpx;
  padding: 20rpx 40rpx 20rpx 40rpx;
  gap: 20rpx;
  border: 3rpx solid #20C38C;
  position: relative;
  top: -26rpx;
  left: 65rpx;
  background-color: white;
}

.mininput {
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
}

.inputBox {
  width: 500rpx;
  float: left;
  height: 100%;
}

.image {
  width: 40rpx;
  height: 40rpx;
  float: right;
  margin-top: 2rpx;
}

.searchRecord {
  margin-top: 26rpx;
}

.mark {
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 32rpx;
  text-transform: capitalize;
  color: #000000;
  margin-left: 62rpx;
  float: left;
}

.deleteMark {
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 24rpx;
  line-height: 34rpx;
  text-transform: capitalize;
  color: #000000;
  opacity: 0.6;
  float: right;
  margin-top: 10rpx;
  margin-right: 70rpx;
}

.markBox {
  width: 622rpx;
  display: flex;
  flex-wrap: wrap;
  position: relative;
  left: 40rpx;
}

.text {
  color: rgba(0, 0, 0, 0.6);
  text-align: center;
  font-size: 30rpx;
  margin-left: 5rpx;
  height: 48rpx;
}

.text1 {
  width: 190rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: rgba(0, 0, 0, 0.6);
  text-align: center;
  font-size: 30rpx;
  margin-left: 5rpx;
  height: 48rpx;
}

.markMake {
  height: 40rpx;
  padding: 4rpx 16rpx;
  border-radius: 148rpx;
  margin-left: 20rpx;
  background: #EBEBEB;
  margin-top: 30rpx;
  z-index: 0;
}

.nonemarkMake {
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 28rpx;
  line-height: 40rpx;
  text-transform: capitalize;
  color: #000000;
  opacity: 0.6;
  margin-top: 18rpx;
  margin-left: 23rpx;
}

.lefticon {
  position: absolute;
  width: 40rpx;
  height: 100rpx;
  left: 676rpx;
  top: 442rpx;
}

.lefticon2 {
  position: absolute;
  top: 345rpx;
  left: 670rpx;
  width: 50rpx;
  height: 50rpx;
  float: left;
  text-align: center;
  z-index: 100;
}

.lefticonImg {
  width: 12rpx;
  height: 40rpx;
  position: relative;
  left: 4rpx;
}

.lefticonImg2 {
  width: 100%;
  height: 100%;
}

.righticon {
  position: relative;
  width: 40rpx;
  height: 100rpx;
  left: 38rpx;
  top: 446rpx;
  transform: matrix(-1, 0, 0, 1, 0, 0);
}

.righticon2 {
  position: relative;
  top: 340rpx;
  left: 590rpx;
  width: 40rpx;
  height: 40rpx;
  text-align: center;
  transform: matrix(-1, 0, 0, 1, 0, 0);
  float: right;
  z-index: 100;
}


.librarySwiper {
  position: absolute;
  width: 504rpx;
  height: 550rpx;
  left: 120rpx;
  top: 222rpx;
}

.libraryIcon1 {
  width: 100%;
  height: 100%;
  margin-top: 72rpx;
  margin-left: 2rpx;
}

.libraryIcon2 {
  width: 500rpx;
  margin-top: 100rpx;
}

.libraryIcon3 {
  width: 500rpx;
  margin-top: 90rpx;
  margin-left: 10rpx;
}

.libraryIcon4 {
  width: 440rpx;
  margin-top: 120rpx;
  margin-left: 40rpx;
}

.library {
  position: absolute;
  top: 460rpx;
}