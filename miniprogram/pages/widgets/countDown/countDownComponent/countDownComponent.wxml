<!--pages/widgets/countDown/countDownComponment/countDownComponent.wxml-->
<!-- 倒计时组件 -->
<view class='countdown'>
  <view class="countdown-txt">
    <text class='cd-txt'>{{countDownTitle}}</text>
  </view>
  <view class="countdownAllBox" bindtap="setTime1">
    <view class="countdownBox" wx:for="{{cdlist3}}" wx:key="index">
      <view class="cdName">
        <view class="cdDay"><text>{{item.countDownEndDate}}</text></view>
        <view class="cdTitle">
          <text class="Postgraduate">{{item.countDownName}}</text>
        </view>
      </view>
    </view>
  </view>
</view>