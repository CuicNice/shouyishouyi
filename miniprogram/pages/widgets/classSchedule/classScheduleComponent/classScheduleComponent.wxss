/* components/classss/classss.wxss */
.dailyScheduleContainer{
  box-sizing: border-box;
  /* Auto layout */
  display: flex;
  flex-direction: column;
  align-items: center;
  background: #FFFFFF;
  padding: 12rpx 0rpx 20rpx;
  width: 500rpx;
  gap: 5rpx;
  /* 边框 */
  border: 2rpx solid #E3E3E6;
  border-radius:13rpx;
}
.dailyScheduleNavigation {
  box-sizing: border-box;
  /* Auto layout */
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  padding: 10rpx;
  width: 100%;
  height: 52rpx;
  /* #EAEAEC */
  border-bottom: 1px solid #EAEAEC;
}

.dailyNavigationtext {
height: 28rpx;
font-family: 'PingFang SC';
font-style: normal;
font-weight: 500;
font-size: 20rpx;
line-height: 28rpx;
/* identical to box height */
/* text */
color: #292945;
}

.dailyScheduleIndexList {
  width: 100%;
  margin-top: 10rpx;
  margin-left: 16rpx;
  margin-right: 16rpx;
  display: flex;
  flex-direction: row;
}

.dailyScheduleTime {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 58rpx;
  height: 72rpx;
}

.dailyScheduleTime1 {
  width: 42rpx;
  height: 28rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 20rpx;
  line-height: 28rpx;
  /* identical to box height */
  color: rgba(41, 41, 69, 0.6);
  margin-top: 16rpx;
}

.dailyScheduleTimeLine {
  width: 24rpx;
  height: 0rpx;
  background: rgba(41, 41, 69, 0.6);
  border: 2rpx solid rgba(41, 41, 69, 0.6);
  transform: rotate(90deg);
  margin-top: 10rpx;
  margin-bottom: 2rpx;
}

.dailyScheduleBox {
  box-sizing: border-box;
  height: 95rpx;
  background: #F9F9F9;
  border: 2rpx solid rgba(216, 216, 216, 0.3);
  border-radius: 8rpx;
  margin-left: 34rpx;
  margin-right: 16rpx;
  width: 390rpx;
  display: flex;
  position: relative;
  z-index: 0;

}

.dailySchedule {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  padding: 0rpx;
  gap: 16rpx;
  width: 206rpx;
  height: 94rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  margin-left: 20rpx;
}

.dailyScheduleClass {
  width: 104rpx;
  height: 32rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 22rpx;
  line-height: 32rpx;
  color: #292945;
}

.dailyScheduleAdBox {
  display: flex;
  flex-direction: row;
  align-items: center;
  gap: 8rpx;
  height: 24rpx;
}

.dailyScheduleAdIcon {
  width: 20rpx;
  height: 20rpx;
  margin-bottom: 32rpx;
}

.dailyScheduleAdIcon image {
  background-image: url("data:image/svg+xml;base64,");
  width: 20rpx;
  height: 20rpx;
}

.havedailyScheduleAdIcon {
  width: 20rpx;
  height: 20rpx;
  margin-bottom: 30rpx;
}

.havedailyScheduleAdIcon image {
  width: 20rpx;
  height: 20rpx;
  background-image: url("data:image/svg+xml;base64,");
}

.dailyScheduleAd {
  width: 120rpx;
  height: 24rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 16rpx;
  line-height: 24rpx;
  /* identical to box height */
  color: rgba(41, 41, 69, 0.6);
  margin-left: 4rpx;
  margin-top: 2rpx;
}

.dailyScheduleKc {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  height: 60rpx;
  margin-top: 14rpx;
  margin-bottom: 14rpx;
  margin-left: 50rpx;
}
.whichKc {
  height: 24rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 16rpx;
  line-height: 24rpx;
  /* identical to box height */
  text-align: right;
  /* text */
  color: #292945;
}

.whichTc {
  width: 52rpx;
  height: 24rpx;
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 400;
  font-size: 16rpx;
  line-height: 24rpx;
  /* identical to box height */
  text-align: right;
  /* text */
  color: #292945;
  margin-top: 18rpx;
}

.dailyScheduleBd {
  position: absolute;
  width: 199.5rpx;
  height: 199.5rpx;
  right: 0rpx;
  top: -4rpx;
  z-index: -1;
}

.dailyScheduleBd image {
  background-image: url("data:image/svg+xml;base64,");
  width: 199.5rpx;
  height: 199.5rpx;
}

.havedailyScheduleBd {
  position: absolute;
  width: 199.5rpx;
  height: 199.5rpx;
  right: 0rpx;
  top: -4rpx;
  z-index: -1;
}

.havedailyScheduleBd image {
  background-image: url("data:image/svg+xml;base64,");
  width: 199.5rpx;
  height: 199.5rpx;
  background-repeat: no-repeat;
}