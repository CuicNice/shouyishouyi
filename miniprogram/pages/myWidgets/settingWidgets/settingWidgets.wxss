/* 背景 */
.bg {
  /* Entering for the first time */
  position: absolute;
  width: 100%;
  height: 100%;
  /* 背景 */
  background: linear-gradient(180.04deg, #B9EFE1 0.04%, #FCFCFC 30%);
  z-index: -999;
}

/* 滑动元素 */
.contant {
  width: 100%;
}

.item-up {
  height: 100rpx;
  width: 80%;
  background: rgb(255, 255, 255);
  border: 2px solid #4BDCAB;
  border-radius: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
  box-shadow: 0px 8px 24px rgba(75, 220, 171, 1);
  z-index: 2;
}

.item-down {
  height: 100rpx;
  width: 80%;
  background: #FFFFFF;
  border: 2px solid #4BDCAB;
  border-radius: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 1;
}

.item-bg-black {
  background-color: rgb(214, 208, 208);
}

.item-bg-write {
  background-color: rgb(255, 255, 255, 0.8);
}

.reminBar {
  position: fixed;
  top: 0;
  z-index: 2;
  width: 100%;
  background-color: red;
}

.TopBar {
  position: fixed;
  background-color: #4BDCAB;
  top: 40px;
  z-index: 2;
}

/* 动画过渡 */
.mask {
  height: 200px;
  background-color: #4BDCAB;
  animation: fadenum 5s ease infinite;
  overflow: hidden;
  opacity: 0;
}

@keyframes fadenum {
  from {
    top: 100px;
    opacity: 100;
  }

  to {
    top: 50px;
    opacity: 0;
  }
}

/* 动画 */
/* 显示或关闭动画*/
.open {
  animation: slideContentUp 0.3s linear both;
}

.close {
  animation: slideContentDown 0.3s linear both;
}

/* 动态设置高度 */
@keyframes slideContentUp {
  from {
    height: 0;
  }

  to {
    height: 80vh;
  }
}

@keyframes slideContentDown {
  from {
    height: 80vh;
  }

  to {
    height: 0;
  }
}

/* 元素滑动结束 */
