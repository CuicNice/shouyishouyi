/* 动画过渡 */

.mask {
    height: 400rpx;
    background-color: #4BDCAB;
    animation: fadenum 5s ease infinite;
    overflow: hidden;
    opacity: 0;
}

@keyframes fadenum {
    from {
        top: 200rpx;
        opacity: 100;
    }
    to {
        top: 100rpx;
        opacity: 0;
    }
}


/* 动画 */


/* 显示或关闭动画*/

.open {
    animation: slideContentUp 0.3s linear both;
}

.close {
    animation: slideContentDown 0.3s linear both;
}


/* 动态设置高度 */

@keyframes slideContentUp {
    from {
        height: 0;
    }
    to {
        height: 80vh;
    }
}

@keyframes slideContentDown {
    from {
        height: 80vh;
    }
    to {
        height: 0;
    }
}


