/* pages/custom-tab-bar/index.wxss */
/* 最外层的Box */
.tabBarBox {
    position: fixed;
    z-index: 9999;
    overflow: visible;
    display: grid;
    width: 100%;
    height: 160rpx;
    background: #FFFFFF;
    box-shadow: 0rpx 0rpx 58rpx rgba(198, 198, 198, 0.6);
    /* color: aqua;
  background-color: aqua; */
    border-radius: 40rpx 40rpx 0rpx 0rpx;
    padding-bottom: constant(safe-area-inset-bottom);
     /* 兼容 iOS < 11.2 */
    /* padding-bottom: env(safe-area-inset-bottom);  */
    /* 兼容 iOS >= 11.2 */
    bottom: 0;
    /* gap: 40px;
  display: flex;
  width: 100%;   */
}

.tapBarContent {
    /* Auto layout */
    overflow: visible;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    /* 、baseline: 成员的第一行文字的基线对齐,  */
    align-items: baseline;
    padding: 0rpx;
    gap: 68rpx;
    position: absolute;
    width: 514rpx;
    height: 160rpx;
    left: 118rpx;
    top: -38rpx;
}
/* 课表 */
.courseTableBox {
    width: 61.72rpx;
    height: 20rpx;
    /* Inside auto layout */
    flex: none;
    order: 1;
    flex-grow: 0;
}

.wedgetsBox {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    padding: 0rpx;
    gap: 10rpx;
    width: 64rpx;
    height: 94rpx;
    /* Inside auto layout */
    flex: none;
    order: 2;
    flex-grow: 0;
}

.wedgetsIconBox {
    width: 64rpx !important;
    height: 94rpx !important;
}