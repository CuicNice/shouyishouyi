<view class="container container-{{type}}" wx:if="{{showOut}}" style="animation-name: {{animationName}};" catchtouchmove="true">
  <view class="binDing-box binDing-box-{{type}}">
    <image wx:if="{{type == 'normal'}}" class='bg-built' src="{{schoolBuiltSrc}}"></image>
    <view class="binDing binDing-{{type}}">
    <view class="dialogDes">
      <view class="bd-text school-text">{{title}}</view>
      <view class="bd-stext">{{stitle}}</view>
    </view>

      <view style="width: 100%;">
        <slot></slot>
      </view>           
    </view>
  </view>
</view>



